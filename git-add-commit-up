#!/bin/bash
p=$(pwd)
last=$p
while [ $p != $HOME ]; do
    last=$p
    git add -A
    git commit -m "update"
    git push $USER $USER
    cd ..
    p=$(pwd)
done
cd blocktree
git fetch $USER
git merge $USER/$USER
cd ..
git add blocktree
echo $last
last=${last/$HOME\//}
echo $last
git add "$last"
if [ -n $(git status -s) ]; then
    git commit -m "update"
    git push $USER $USER
    cd ..
    git add $USER
    git commit -m "update"
    git push $USER $USER
fi
